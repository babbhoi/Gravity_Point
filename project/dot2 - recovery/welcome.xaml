    <Grid>
        <Canvas HorizontalAlignment="Left" Height="640" VerticalAlignment="Top" Width="400" Tapped="Canvas_Tapped">
            <Image x:Name="G" Height="200" Canvas.Left="100" Canvas.Top="100" Width="200" Source="Assets/g.png" RenderTransformOrigin="0.5,0.5">
            	<Image.RenderTransform>
            		<CompositeTransform/>
            	</Image.RenderTransform>
            </Image>
            <Image x:Name="point" Height="200" Canvas.Left="100" Canvas.Top="100" Width="200" Source="Assets/point.png" RenderTransformOrigin="0.5,0.5">
            	<Image.RenderTransform>
            		<CompositeTransform/>
            	</Image.RenderTransform>
            </Image>
            <Image x:Name="gravity" Height="200" Canvas.Left="62" Canvas.Top="290" Width="200" Source="Assets/gravity.png" RenderTransformOrigin="0.5,0.5">
            	<Image.RenderTransform>
            		<CompositeTransform/>
            	</Image.RenderTransform>
            </Image>
            <Image x:Name="pint" Height="200" Canvas.Left="120" Canvas.Top="351" Width="200" Source="Assets/p_int.png" RenderTransformOrigin="0.5,0.5">
            	<Image.RenderTransform>
            		<CompositeTransform/>
            	</Image.RenderTransform>
            </Image>
        </Canvas>
        <MediaElement x:Name="media" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Visibility="Collapsed" AudioCategory="BackgroundCapableMedia" Source="Assets/start.wav"/>
    </Grid>
</Page>
